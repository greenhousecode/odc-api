{
  "name": "odc-api",
  "version": "1.2.5",
  "description": "API that simplifies working with the WPP ODC platform",
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c && tsc",
    "lint": "eslint  --ignore-path .gitignore '*/**/*.{js,ts,json}' --quiet --fix",
    "prepublish": "npm run build",
    "format": "prettier --ignore-path .gitignore --check '**/*.{js,ts,json}'"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.browser.js",
  "types": "dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "author": "MEH <meh@greenhousegroup.com>",
  "license": "ISC",
  "lint-staged": {
    "*.js": [
      "yarn lint --fix",
      "git add"
    ],
    "*.{css,graphql,html,json,md,scss,yml}": [
      "yarn format --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/express": "^4.17.6",
    "@types/node": "^14.11.10",
    "@types/triple-beam": "^1.3.1",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@yelo/rollup-node-external": "^1.0.1",
    "browser-sync": "^2.26.7",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.1",
    "prettier": "^2.1.2",
    "rollup": "^2.30.0",
    "rollup-plugin-typescript2": "^0.28.0",
    "ts-node": "^9.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "deep-equal": "^2.0.4",
    "form-data": "^3.0.0"
  }
}
